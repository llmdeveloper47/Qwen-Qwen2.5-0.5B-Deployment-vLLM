{
  "name": "BitsAndBytes INT8 Configuration",
  "description": "8-bit quantization using bitsandbytes. Good balance of speed, memory, and accuracy.",
  "environment": {
    "MODEL_NAME": "codefactory4791/intent-classification-qwen",
    "MAX_MODEL_LEN": "512",
    "QUANTIZATION": "bitsandbytes",
    "TRUST_REMOTE_CODE": "true",
    "BATCH_SIZE": "16",
    "USE_BETTER_TRANSFORMER": "false",
    "USE_COMPILE": "false"
  },
  "gpu": {
    "type": "A100-SXM4-40GB",
    "min_workers": 0,
    "max_workers": 1
  },
  "expected_performance": {
    "vram_usage_gb": "~2-2.5",
    "latency_p95_ms": "~70-110",
    "throughput_samples_s": "~80-95",
    "accuracy_degradation": "<1%"
  },
  "use_case": "Memory-constrained deployments. Enables running on smaller GPUs or higher concurrency."
}

